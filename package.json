{
  "name": "atom-space-pen-views",
  "version": "2.2.0",
  "description": "Atom SpacePen views that used to live in core.",
  "main": "./lib/main",
  "files": [
    "lib/**",
    "src/**",
    "stylesheets/**"
  ],
  "scripts": {
    "test": "atom --test spec",
    "clean": "shx rm -rf lib",
    "build": "npm run clean && shx cp -r src lib && coffee -c -M -o lib lib && shx rm -r lib/*.coffee",
    "prepare": "npm run build"
  },
  "atomTestRunner": "./runner",
  "repository": {
    "type": "git",
    "url": "https://github.com/atom/atom-space-pen-views.git"
  },
  "bugs": {
    "url": "https://github.com/atom/atom-space-pen-views/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/atom/atom-space-pen-views/raw/master/LICENSE.md"
    }
  ],
  "dependencies": {
    "fuzzaldrin": "^2.1.0",
    "space-pen-plus": "^6.0.3"
  },
  "devDependencies": {
    "atom-jasmine3-test-runner": "^4.4.7",
    "jasmine": "^3.5.0",
    "@types/jasmine": "^3.5.10",
    "coffee-cache": "^1.0.2",
    "coffeescript": "^1.12.7",
    "jasmine-focused": "^1.0.7",
    "rimraf": "^3.0.2",
    "temp": "^0.9.1"
  }
}
